<script>
$(document).ready(function(e){
    init(e);
    function renderAll(){
        var fashion = getFashionBySlug('quinte-lookbook').fashion_images;
        renderFashion("#fashion_banner", "#fashion_banner_template", fashion);
        
        setTimeout(function() {
            show_content();
        }, 500);
    
        $('.fashion_flexslider').flexslider({
            animation: "slide",
        });
    }
    loadMallDataCached(renderAll); 
})
